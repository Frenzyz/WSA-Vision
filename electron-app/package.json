{
  "name": "wsa-electron-app",
  "version": "1.0.0",
  "description": "Electron UI for WSA",
  "main": "main.js",
  "scripts": {
    "start": "webpack && electron .",
    "build-go": "go build -o go_build_WSA_WSA ../WSA-Vision/main.go",
    "copy-go": "cp ../WSA-Vision/go_build_WSA_WSA* ./",
    "build": "npm run build-go && npm run copy-go && webpack && electron-builder"
  },
  "author": "Aadit Chetan",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "electron": "^25.2.0",
    "electron-builder": "^24.6.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "build": {
    "appId": "com.example.wsa",
    "files": [
      "**/*",
      "go_build_WSA_WSA*"
    ],
    "extraFiles": [
      "go_build_WSA_WSA*"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
